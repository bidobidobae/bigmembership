<tr id="<%= dom_id user %>" class="bg-white hover:bg-gray-50 focus:bg-gray-300 active:bg-red-200" tabindex="0">
  <td class="border px-2 py-2"><%= user.first_name %></td>
  <td class="border px-2 py-2"><%= user.last_name %></td>
  <td class="border px-2 py-2"><%= user.phone %></td>
  <td class="border px-2 py-2"><%= user.email %></td>
  <td class="border px-2 py-2"><%= user.status_label %></td>
  <td class="border px-2 py-2"><%= user.active_date %></td>
  <td class="border px-2 py-2"><%= user.expired_date %></td>  
  <td class="border px-2 py-2"><%= user.point %></td>  
  <td class="border px-2 py-2"><%= link_to "Edit", edit_user_path(user), class: "text-blue-600", data: { action: "click->modal#open", turbo_stream: "" } %></td>
</tr>

